
@{
    ViewData["Title"] = "Home";
    Layout = "~/Views/_shared/_layout.cshtml";
}

@using project_banhang.Models;

@{
    DataContext db_category = new DataContext();
    DataContext db_subcategory = new DataContext();
    DataContext db_itemsList = new DataContext();
    var itemscategory = db_category.ItemsCategories.OrderByDescending(tbl => tbl.id).ToList();
    var _itemsList = db_itemsList.ItemsList.OrderByDescending(tbl => tbl.id).ToList();
}

@foreach (var item in itemscategory)
{
    @if (item.showhome == true)
    {
        <section class="awe-section-4" id="home_block_4">
            <div class="section_fashion_hot section_base ">
                <div class="container">
                    <div class="row">
                        <div class="clearfix">
                            <div class="col-md-12">
                                <div class="border_bottom_title clearfix">
                                </div>
                                <div class="title_top_menu">

                                    <h3><a href="/Product/Category/@item.id" title="@item.name">@item.name</a></h3>
                                    <div class="btn_menu">
                                        <i class="fa fa-ellipsis-h" aria-hidden="true"></i>
                                    </div>
                                    <ul class="clearfix">

                                        @if (item.id != 10)
                                        {
                                            <li><a href="/Product/Category/@item.id?storage=(120GB → 128GB)" title="@item.name 120GB – 128GB">@item.name 120GB – 128GB</a></li>
                                            <li><a href="/Product/Category/@item.id?storage=(240GB → 256GB)" title="@item.name 240GB – 250GB – 256GB">@item.name 240GB - 256GB</a></li>

                                            <li><a href="/Product/Category/@item.id?storage=(480GB → 512GB)" title="@item.name 480GB – 500GB – 512GB">@item.name 480GB – 512GB</a></li>

                                            <li><a href="/Product/Category/@item.id?storage=(960GB → 1024GB)" title="@item.name 960GB – 1tb">@item.name 960GB – 1TB</a></li>
}
                                        else
                                        {
                                            <li><a href="/Product/Category/@item.id?storage=(2048GB → 4096GB)" title="@item.name 120GB – 128GB">@item.name 2GB - 4GB</a></li>
                                            <li><a href="/Product/Category/@item.id?storage=(4096GB → 6144GB)" title="@item.name 240GB – 250GB – 256GB">@item.name 4GB - 6GB</a></li>

                                            <li><a href="/Product/Category/@item.id?storage=(6144GB → 10240GB)" title="@item.name 480GB – 500GB – 512GB">@item.name 6GB - 10GB</a></li>
                                        }

                                        <li><a href="/Product/Category/@item.id" class="more_view">Xem tất cả</a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="content_sec clearfix ">
                                    <div class="col-md-9 col-lg-9 image_right col-lg-push-3 col-md-push-3">
                                        <div class="prd_sec">
                                            <div class="products owl-carousel owl-theme products-view-grid owl-loaded owl-drag" data-nav="true" data-lg-items="4" data-md-items="3" data-sm-items="3" data-xs-items="2" data-margin="15">
                                                <!-- ADD DIV -->
                                                <div class="owl-stage-outer">
                                                    <div class="owl-stage" style="transform: translate3d(0px, 0px, 0px); transition: all 0s ease 0s; width: 1340px;">

                                                        @foreach (var _item in _itemsList)
                                                        {
                                                            if (item.id == _item.id_category)
                                                            {
                                                                <div class="owl-item active" style="width: 208.25px; margin-right: 15px;">
                                                                    <div class="product-box-h">
                                                                        <div class="product-thumbnail">
                                                                            <a class="image_link display_flex" href="/Product/Detail/@_item.id" title="@_item.name">
                                                                                <img src="~/Upload/Products/@_item.media" data-lazyload="~/Upload/Products/@_item.media" alt="@_item.name" style="max-height:200px;max-width:200px">
                                                                            </a>

                                                                            <!-- Tag giá sốc -->
                                                                            @if (_item.sale == true)
                                                                            {<text>
                                                                                    <div class="tagdacbiet_sale sale-flash">
                                                                                        <div class="font16">-@_item.discount %</div>
                                                                                    </div></text>}

                                                                            <!-- End tag giá sốc -->
                                                                            <!-- Tag Mã KM -->
                                                                            <!-- End Tag Mã KM -->
                                                                        </div>
                                                                        <div class="product-info a-left">
                                                                            <h3 class="product-name"><a class="height_name text2line" href="/Product/Detail/@_item.id" title="@_item.name">@_item.name</a></h3>

                                                                            <div class="product-hides">
                                                                                <div class="product-hide">
                                                                                    @{
                                                                                        var infor = System.Globalization.CultureInfo.GetCultureInfo("vi-VN");
                                                                                    }

                                                                                    <div class="price-box clearfix">
                                                                                        @if (_item.sale == true)
                                                                                        {
                                                                                            double newprice = _item.price - _item.price * _item.discount / 100;
                                                                                            <text><div class="special-price">
                                                                                                    <span class="price product-price">@string.Format(infor, "{0:c0" + "}", newprice)</span>
                                                                                                </div>
                                                                                                <div class="old-price">
                                                                                                    <span class="price product-price-old">
                                                                                                        @string.Format(infor, "{0:c0" + "}", _item.price)
                                                                                                    </span>
                                                                                                </div></text>
                                                                                        }
                                                                                        else
                                                                                        {
                                                                                            <text><div class="old-price">
                                                                                                    <span class="price product-price">
                                                                                                        @string.Format(infor, "{0:c0" + "}", _item.price)
                                                                                                    </span>
                                                                                                </div></text>
                                                                                        }


                                                                                    </div>


                                                                                </div>
                                                                            </div>
                                                                            <div class="product-action clearfix hidden-xs">
                                                                                <form action="/Cart/Buy/@_item.id" method="post" class="variants form-nut-grid" data-id="@_item.id" enctype="multipart/form-data">
                                                                                    @Html.AntiForgeryToken()
                                                                                    <div>

                                                                                        <input type="hidden" name="variantId" value="@_item.id">
                                                                                        <input type="hidden" name="quantity" value="1">
                                                                                        <button class="btn-buy btn-cart btn btn-circle left-to " title="Thêm vào giỏ hàng">
                                                                                            <i class="fa fa-shopping-basket"></i>Mua ngay
                                                                                        </button>


                                                                                        <a title="Xem nhanh" href="/Product/quickViewProduct/@item.id" data-handle="@item.name" class="xem_nhanh btn-circle btn_view btn right-to quick-view hidden-xs hidden-sm hidden-md"
                                                                                           data-original-title="Xem nhanh">
                                                                                            <i class="fa fa-eye"></i>
                                                                                        </a>

                                                                                    </div>
                                                                                </form>
                                                                            </div>

                                                                            <div class="bizweb-product-reviews-badge" data-id="12666360">
                                                                                <div class="bizweb-product-reviews-star" data-score="4.7" data-number="5" title="Tuyệt vời" style="color: rgb(255, 190, 0);">
                                                                                    <i data-alt="1" class="star-on-png" title="Tuyệt vời"></i>&nbsp;<i data-alt="2" class="star-on-png" title="Tuyệt vời"></i>&nbsp;<i data-alt="3" class="star-on-png" title="Tuyệt vời"></i>&nbsp;
                                                                                    <i data-alt="4" class="star-on-png" title="Tuyệt vời"></i>&nbsp;<i data-alt="5" class="star-half-png" title="Tuyệt vời"></i><input name="score" type="hidden" value="4.7" readonly="">
                                                                                </div>
                                                                                <div>
                                                                                    <p>3</p>
                                                                                </div>
                                                                                <div><img src="~/Memory-Home/images/user.png" width="18" height="17"></div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            }
                                                        }
                                                        
                                                    </div>
                                                </div>
                                                <div class="owl-nav">
                                                    <div class="owl-prev disabled"> </div>
                                                    <div class="owl-next"> </div>
                                                </div>
                                                <div class="owl-dots disabled"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3 hidden-sm  fix-padding-left col-lg-pull-9 col-md-pull-9">
                                        <div class="banner_img">
                                            <a href="#" class="f-left wid_fix padding-bottom-30">
                                                <img src="~/Memory-Home/images/banner_1_fashion.png" data-lazyload="//bizweb.dktcdn.net/100/329/122/themes/681013/assets/banner_1_fashion.png?1574878515674" alt="Mua SSD tặng cáp Goflex, bộ Vít đa năng hoặc Case đỡ ổ cứng cho PC" title="Mua SSD tặng cáp Goflex, bộ Vít đa năng hoặc Case đỡ ổ cứng cho PC"
                                                     class="img-responsive center-base">
                                            </a>
                                            <a href="/Product/Category/@item.id">
                                                <img src="~/Upload/Category-Thum/@item.thumnails" data-lazyload="~/Upload/Category-Thum/@item.thumnails" title="@item.name" class="img-responsive center-base">
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    }
}



